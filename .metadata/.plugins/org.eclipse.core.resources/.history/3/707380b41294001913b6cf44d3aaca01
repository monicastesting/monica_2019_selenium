package jabaBasicProg;

public class str {

	
	public static String int3050(String str)
	{   
		if(str.length()<=3 && str.equalsIgnoreCase("del") )
		{
			return str;
		}
		int index=str.indexOf("del");
		string[]arr= str.split(index);
		 return arr;
			
		}
		//string rs=str.replace("del","");
		
		
	}
	public static void main(String[] args) {
		
		string res=int3050("adelasa");
		System.out.println(res);
	}

}
